# About predicates
# Not differentiating surface predicates and abstract in this.

eds:Pos
 a rdfs:Class .

eds:hasLemma
 a rdf:Property ;
 rdfs:domain eds:NodePredicate ;
 rdfs:range rdfs:Literal .

eds:hasPos
 a rdf:Property ;
 rdfs:domain eds:NodePredicate ;
 rdfs:range eds:Pos .

eds:hasSense
 a rdf:Property ;
 rdfs:domain eds:NodePredicate ;
 rdfs:range rdfs:Literal .

# About the possible arcs/edges of the graph:

eds:bv
 a rdf:Property ;
 rdfs:subPropertyOf eds:edge ;
 rdfs:domain eds:Node ;
 rdfs:range eds:Node .

eds:arg1
 a rdf:Property ;
 rdfs:subPropertyOf eds:edge ;
 rdfs:domain eds:Node ;
 rdfs:range eds:Node .

eds:arg2
 a rdf:Property ;
 rdfs:subPropertyOf eds:edge ;
 rdfs:domain eds:Node ;
 rdfs:range eds:Node .

eds:arg3
 a rdf:Property ;
