 rdfs:range dmrs:Predicate .

dmrs:hasSortInfo #?
 a rdf:Property ;
 rdfs:domain dmrs:Node ;
 rdfs:range rdfs:Literal .

dmrs:cfrom
 a rdf:Property ;
 rdfs:domain dmrs:Node ;
 rdfs:range rdfs:Literal .
 
dmrs:cto
 a rdf:Property ;
 rdfs:domain dmrs:Node ;
 rdfs:range rdfs:Literal .

dmrs:hasCarg
 a rdf:Property ;
 rdfs:domain dmrs:Node ;
 rdfs:range rdfs:Literal .


