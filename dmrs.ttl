@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dmrs: <http://www.delph-in.net/schema/dmrs#> .
@prefix dmrstype: <http://www.delph-in.net/schema/dmrs#type#> .
@prefix erg: <http://www.delph-in.net/schema/erg#> .

dmrs:DMRS
 a rdfs:Class ;
 rdfs:comment "Has nodes and links"@en-us .

dmrs:text
 a rdfs:Property ;
 rdfs:domain dmrs:DMRS ;
 rdfs:range rdfs:Literal .
 
dmrs:Node
 a rdfs:Class ;
 rdfs:comment ""@en-us .

dmrs:NodeType
 a rdfs:Class ;
 rdfs:comment ""@en-us .

dmrs:Link
 a rdfs:Class ;
 rdfs:comment ""@en-us .

dmrs:ScopalRelation
 a rdfs:Class .

dmrs:Role
 a rdfs:Class .

dmrs:Predicate
 a rdfs:Class ;
 rdfs:comment ""@en-us .

dmrs:AbstractPredicate
 a rdfs:Class ;
 rdfs:subClassOf dmrs:Predicate;
 rdfs:comment ""@en-us .

dmrs:SurfacePredicate
 a rdfs:Class ;
 rdfs:subClassOf dmrs:Predicate;
 rdfs:comment ""@en-us .

dmrs:Pos
 a rdfs:Class .

dmrs:hasTop
 a rdf:Property ;
 rdfs:domain dmrs:DMRS ;
 rdfs:range dmrs:Node .

dmrs:hasIndex
 a rdf:Property ;
 rdfs:domain dmrs:DMRS ;
 rdfs:range dmrs:Node .

dmrs:hasNode
 a rdf:Property ;
